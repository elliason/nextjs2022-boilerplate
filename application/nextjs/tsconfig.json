{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "target": "es2020",
        "noEmit": true,
        "module": "esnext",
        "moduleResolution": "node",
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
