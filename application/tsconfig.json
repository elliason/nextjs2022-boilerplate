{
    "extends": "@media-factory-cz/typescript-config",
    "compilerOptions" : {
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "sourceMap" : true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"],
    "exclude": ["node_modules"]
}
